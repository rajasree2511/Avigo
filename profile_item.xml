<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_margin="12dp"    android:gravity="center_vertical"    android:orientation="horizontal">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="center_vertical"        android:orientation="horizontal">        <LinearLayout            android:id="@+id/backImg"            android:layout_width="46dp"            android:layout_height="46dp"            android:background="#999999"            android:gravity="center">            <ImageView                android:id="@+id/iconIv"                android:layout_width="24dp"                android:layout_height="24dp"                android:src="@drawable/baseline_chevron_left_24" />        </LinearLayout>        <TextView            android:id="@+id/tilteTv"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="12dp"            android:fontFamily="@font/montserratsemibold"            android:textColor="@color/black"            android:textSize="16sp" />    </LinearLayout>    <ImageView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_gravity="end|center"        android:rotation="180"        android:src="@drawable/baseline_chevron_left_24"        app:tint="@color/black" /></FrameLayout>